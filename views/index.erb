<!doctype html>
<html>
  <head>
    <title><%= title %></title>
    <meta name="author" content="<%= author %>">
    <meta charset="utf-8">
    <link rel="shortcut icon" href="favicon.ico">

    <link href="http://fonts.googleapis.com/css?family=Lato:700|Merriweather:400,700,300|Oswald:400,700" rel="stylesheet" type="text/css">

    <!-- http://lesscss.org/ -->
    <link href="print.less" media="print" rel="stylesheet/less" type="text/css" />
    <link href="style.less" media="screen" rel="stylesheet/less" type="text/css" />
    <script src="less-1.3.0.min.js" type="text/javascript"></script>

    <script type="text/javascript">
      var _gaq = _gaq || [];
      _gaq.push(['_setAccount', '<%= key %>']);
      _gaq.push(['_trackPageview']);

      (function() {
        var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
        ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
        var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
      })();
    </script>
  </head>
  <body>
    <div id="resume">
      <%= resume %>

      <small class="download">Download as <a href="resume.txt">plain text</a>.</small>
    </div>
  </body>
</html>
